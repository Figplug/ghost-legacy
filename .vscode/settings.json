{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "editor.rulers": [90],
    "editor.formatOnSave": true,
  },

  "editor.defaultFormatter": null,
  "prettier.tabWidth": 2,
  "prettier.useTabs": true,
  "prettier.singleQuote": true,
  "prettier.semi": false,
}