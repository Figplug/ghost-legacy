{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "noEmit": false,  // Emit handled by webpack.
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
